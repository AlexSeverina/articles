digraph G {
rankdir = LR
node [shape = circle]
0[style=filled, fillcolor=green]
5[shape = doublecircle, style=filled, fillcolor=red]
{ rank=same; 0 }
{ rank=min; 0 }
{ rank=same; 5 }
{ rank=max; 5 }
0 -> 1 [label="b"]; 
1 -> 3 [label="n"]; 
1 -> 2 [label="c"]; 
3 -> 3 [label="m"]; 
3 -> 4 [label="l"]; 
2 -> 6 [label="n"]; 
6 -> 6 [label="m"]; 
6 -> 5 [label="m"]; 
4 -> 5 [label="m"]; 

}
